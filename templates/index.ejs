    <header id="home">
        <div class="header-content">
            <h2>Indulge your senses with our fragrant laundry experience</h2>
            <div class="line"></div>
            <h1 >Order your first pickup</h1>
            <a href="#events" class="ctn" onclick='removeall(); $("#quad").css("border", "2px solid whitesmoke"); $("#quad").css("border-radius", "20px");'>Learn more</a>
        </div>
    </header>

     <!--Events-->
     <section class="events" id="events">
        <div class="container">
        <div class="title">
            <h1 class="dark">Our Services</h1>
            <div class="line"></div>
        </div>
        <div class="row">
            <article class="card col">
                <img class="card-img" src="./img/img1.jpg" width="500" height="600">
